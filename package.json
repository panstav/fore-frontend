{
  "name": "fore-frontend",
  "version": "0.9.4",
  "description": "",
  "type": "module",
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "NODE_ENV",
      "API",
      "npm_package_version"
    ]
  },
  "scripts": {
    "start-offline": "npm run build-entry-dev; NODE_ENV=offline parcel src/index.html",
    "start-dev-local": "npm run setup-dev; API=local parcel src/index.html",
    "start-dev-github": "npm run setup-dev; API=github parcel src/index.html",
    "start-dev-cloud": "npm run setup-dev; API=cloud parcel src/index.html",
    "start-production": "npm run build-entry-prod; parcel src/index.html",
    "build": "npm run build-entry-prod; parcel build --no-source-maps src/index.html; cp static/* dist",
    "clean": "rm dist/* -rf; rm .parcel-cache/* -rf",
    "lint": "eslint .",
    "test": "npm run lint",
    "fake-data": "node scripts/fake-data.js",
    "preversion": "npm test",
    "setup-dev": "npm run clean; npm run build-entry-dev; export NODE_ENV=development",
    "build-entry-dev": "node scripts/build-entry-dev.js",
    "build-entry-prod": "node scripts/build-entry-prod.js; export NODE_ENV=production"
  },
  "keywords": [],
  "author": "Stav Geffen <stavgeffen@gmail.com> (https://github.com/panstav)",
  "license": "ISC",
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@sentry/react": "^7.17.4",
    "@sentry/tracing": "^7.17.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "bulma": "^0.9.3",
    "bulma-pageloader": "^0.3.0",
    "classnames": "^2.3.1",
    "dateformat": "^5.0.2",
    "htm": "^3.1.0",
    "javascript-time-ago": "^2.3.10",
    "ky": "^0.28.7",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "parcel": "^2.2.1",
    "posthtml-expressions": "^1.9.0",
    "posthtml-minifier": "^0.1.0",
    "preact": "^10.6.4",
    "preact-router": "^3.2.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.25.3",
    "react-modal": "^3.14.4",
    "react-toastify": "^9.0.1",
    "tippy.js": "^6.3.7",
    "unistore": "^3.5.2",
    "wouter-preact": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "parcel-resolver-ignore": "^2.1.3",
    "process": "^0.11.10"
  }
}
