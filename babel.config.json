{
	"env": {
		"production": {
			"plugins": [
				["module-resolver", {
					"root": ["./src", "./src/components", "./src/components/common"],
					"alias": {
						"preact/debug": "/lib/noop",
						"unistore/devtools": "/lib/noop"
					}
				}]
			]
		},
		"offline": {
			"plugins": [
				["module-resolver", {
					"root": ["./src", "./src/components", "./src/components/common"],
					"alias": {
						"services/xhr": "./src/services/xhr/fixtures",
						"fixtures": "./fixtures"
					}
				}]
			]
		}
	},

	"plugins": [
		["@babel/plugin-transform-react-jsx", {
			"runtime": "automatic",
			"importSource": "preact"
		}],
		["module-resolver", {
			"root": ["./src", "./src/components"]
		}]
	]
}