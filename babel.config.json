{
	"env": {
		"production": {
			"plugins": [
				["module-resolver", {
					"root": ["./src", "./src/components"],
					"alias": {
						"preact/debug": "/lib/noop",
						"unistore/devtools": "/lib/noop"
					}
				}]
			]
		}
	},

	"plugins": [
		["@babel/plugin-transform-react-jsx", {
			"runtime": "automatic",
			"importSource": "preact"
		}],
		["module-resolver", {
			"root": ["./src", "./src/components"],
			"alias": {
				"services/xhr": "./src/services/xhr/fixtures",
				"fixtures": "./fixtures"
			}
		}]
	]
}